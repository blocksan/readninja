<md-toolbar class="md-whiteframe-2dp md--search--toolbar">
    <div class="container">
        <div class="row md-toolbar-tools">
            <div class="col-lg-2 li--tabs" ui-sref="home.dashboard">
                <md-truncate class="logo--name"><img src="/img/Logomakr_8VnYBN.png" alt="" style="width:140px"></md-truncate>
            </div>

            <div class="col-lg-2 li--tabs tutorials" ng-click="toggleSearch(true)" ui-sref="home.filter">Tutorials

            </div>
            <div class="col-lg-2 li--tabs" ui-sref="home.newpost">Articles</div>
            <div class="col-lg-2 li--tabs searchbar--button">
                <button aria-label="search item" ng-click="toggleSearch()">
                        <i class="fa fa-search" ng-if="search_toggle" aria-hidden="true"></i>
                        <i class="fa fa-times" ng-if="!search_toggle" aria-hidden="true"></i>
                        <svg width="50" height="50"
                             viewBox="0 0 50 50" version="1.1"
                             xmlns="http://www.w3.org/2000/svg"
                             xmlns:xlink="http://www.w3.org/1999/xlink" ng-if="!search_toggle" class="drop--down--svg">
                            <polyline points="0 0,25 25,50 0" style="fill:#dedede"/>
                        </svg>
                    </button>
            </div>
            <div class="col-lg-1 li--tabs">Authors</div>
            <div class="col-lg-3 li--tabs signup" ng-if="!logged">
                <md-button class="md-raised " aria-label="Learn More" ng-click="showPrerenderedDialog($event)">
                    Login
                </md-button>
                <md-button class="md-raised md-primary" ui-sref="home.profile.posts" aria-label="Learn More">
                    Join Us
                </md-button>
            </div>
            <div class="col-lg-3 li--tabs profile--button" ng-if="logged">

                <div class="profile--image--tab" aria-label="Learn More" ui-sref="home.profile.posts">
                    <img src="{{user.avatar}}" alt=""> Profile
                </div>
                <md-button class="md-raised" ng-click="logout()" aria-label="Learn More">
                    Logout
                </md-button>
            </div>

